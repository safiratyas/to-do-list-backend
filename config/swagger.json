              "application/json": {
                "schema": {
                  "$ref": "#/definitions/coreList"
                }
              }
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "tags": [
          "List"
        ]
      }
    }
