        "parameters": [{
          "name": "id",
          "in": "path",
          "required": true,
          "type": "integer",
          "format": "int64"
        }],
        "tags": [
          "User"
        ]
      }
    },
    "/api/users": {
      "get": {
        "summary": "Get All Users",
        "description": "Can Get Users, But Can not Edit and Delete",
        "security": [{
          "BearerTokenAuth": []
        }],
        "responses": {
          "200": {
            "description": "Berhasil ditampilkan",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/definitions/coreUser"
